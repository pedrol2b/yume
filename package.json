{
  "name": "yume",
  "version": "0.1.1",
  "description": "A comprehensive mental health tool combining evidence-based breathing techniques with grounding exercises, designed specifically for anxiety management and panic attack situations. Features customizable breathing patterns, calming mantras, and the 5-4-3-2-1 grounding technique to promote mindfulness and emotional regulation.",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "pretty": "prettier --write \"./**/*.{js,ts,json}\"",
    "pwa:build": "next build && npx cap sync",
    "pwa:dev": "next dev",
    "android:setup": "npx cap add android",
    "android:sync": "npx cap sync android",
    "android:clean": "cd android && ./gradlew clean",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:build-debug": "cd android && ./gradlew assembleDebug",
    "android:run": "npx cap run android",
    "apk:setup": "npm run android:setup && npm run keystore:create",
    "apk:build": "npm run pwa:build && npm run android:build && npm run apk:copy",
    "apk:build-debug": "npm run pwa:build && npm run android:build-debug && npm run apk:copy-debug",
    "apk:copy": "cp android/app/build/outputs/apk/release/app-release.apk ./yume-release.apk",
    "apk:copy-debug": "cp android/app/build/outputs/apk/debug/app-debug.apk ./yume-debug.apk",
    "apk:full-build": "npm run apk:build && npm run apk:build-debug",
    "keystore:create": "cd android/app && keytool -genkey -v -keystore debug.keystore -storepass android -alias androiddebugkey -keypass android -keyalg RSA -keysize 2048 -validity 10000 -dname 'CN=Android Debug,O=Android,C=US' 2>/dev/null || echo 'Keystore already exists'",
    "capacitor:init": "npx cap init",
    "capacitor:add-android": "npx cap add android",
    "capacitor:sync": "npx cap sync",
    "capacitor:build": "npx cap build android",
    "deploy:android": "npm run apk:full-build && echo 'APKs built successfully! Install yume-release.apk on your device.'"
  },
  "keywords": [
    "mental health",
    "anxiety relief",
    "panic attack",
    "breathing exercise",
    "guided breathing",
    "478 breathing",
    "grounding technique",
    "54321 method",
    "mindfulness",
    "relaxation",
    "calming app",
    "stress relief",
    "meditation",
    "affirmations",
    "mantras",
    "emotional regulation",
    "self care",
    "pwa app",
    "offline support",
    "dark mode"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrol2b/yume.git"
  },
  "author": "Pedro Bueno <pedrolbb.e@gmail.com> (https://github.com/pedrol2b)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@next/eslint-plugin-next": "^15.5.3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.18.4",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.57.0",
